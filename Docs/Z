actions => [ qw( +current +next ~done ~later ) ],
nexts   => [ qw(          +next ~done ~later ) ],
dones   => [ qw( +done ) ],   
lates   => [ qw( +late ~done ) ],
tickles => [ qw( +tickle ~done ) ],
slow    => [ qw( +slow ~done ) ],
ideas   => [ qw( +idea ~done ) ],
due     => [ qw( +due ~done ~later ) ],
someday => [ qw( +someday ~done ) ],
future  => [ qw( +someday +tickle ~done ) ],


